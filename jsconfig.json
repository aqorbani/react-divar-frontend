{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "src/*": ["src/*"],
      "assets/*": ["src/assets/*"],
      "components/*": ["src/components/*"],
      "configs/*": ["src/configs/*"],
      "pages/*": ["./src/pages/*", "./src/pages/"],
      "layouts/*": ["src/layouts/*"],
      "router/*": ["src/router/*"],
      "services/*": ["src/services/*"],
      "styles/*": ["src/styles/*"],
      "utils/*": ["src/utils/*"]
    }
  },
  "include": ["./"],
  "exclude": ["node_modules", "build"]
}
